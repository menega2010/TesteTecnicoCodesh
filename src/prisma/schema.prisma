generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tab_user {
  id       String @id @default(uuid())
  name     String
  lastName String @map("last_name")
  password     String
  phone    String @unique

  createdAt String     @map("created_at") @db.VarChar(32)
  updatedAt String?    @map("updated_at") @db.VarChar(32)
}

